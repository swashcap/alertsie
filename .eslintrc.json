{
  "extends": "airbnb-base",
  "plugins": [
    "import"
  ],
  "rules": {
    "comma-dangle": [
      "error",
      {
        "arrays": "always-multiline",
        "objects": "always-multiline",
        "functions": "ignore"
      }
    ],
    "no-restricted-properties": [
      0,
      {
        "object": "Math",
        "property": "pow"
      }
    ],
    "strict": [0, "error"]
  }
}
